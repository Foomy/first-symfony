
{%  block toggle_widget %}
	<input type="checkbox"
	       id="{{ id }}"
	       data-toggle="toggle"
	       data-width="{{ width }}"
	       data-height="{{ height }}"
	       data-onstyle="{{ styleon }}"
	       data-offstyle="{{ styleoff }}"
	       data-on="{{ choiceon }}"
	       data-off="{{ choiceoff }}">
{% endblock %}